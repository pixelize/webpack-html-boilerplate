{
  "name": "webpack-html-boilerplate",
  "version": "1.0.0",
  "description": "A simple webpack html boilerplate including some tools for your front-end development",
  "scripts": {
    "start": "babel-node scripts/server.js",
    "build": "babel-node ./node_modules/.bin/webpack",
    "lint": "snazzy",
    "test": "npm run lint && npm run build",
    "email:watch": "concurrently 'mjml -w email/email.mjml -o email/email.html' 'live-server email'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pixelize/webpack-html-boilerplate"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "mjml"
  ],
  "author": "Gauthier Eloy",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "concurrently": "^2.1.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "file-loader": "^0.8.5",
    "fontgen-loader": "^0.1.10",
    "html-loader": "^0.4.0",
    "jquery": "^2.2.4",
    "live-server": "^1.0.0",
    "lost": "^6.7.1",
    "mjml": "^2.1.4",
    "morgan": "^1.6.1",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "sass-loader": "^3.1.2",
    "snazzy": "^2.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  }
}
